@Library('my-tf-library@main') _

pipeline{
    agent any
    stages {
       stage('Checkout'){
          steps {
            codeCheckout()
    }
}
stage('tfinit'){
    steps {
        initTerraform()
    }
}
stage('tfvalidate'){
    steps {
        validateTerraform()
    }
}
stage('plan'){
    steps {
        planTerraform()
    }
}
}
}
